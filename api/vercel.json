{
  "version": 2,
  "functions": {
    "src/http/server.ts": {
      "runtime": "nodejs20.x",
      "includeFiles": [
        "prisma/**",
        "node_modules/.prisma/client/**",
        "node_modules/@prisma/engines/**"
      ]
    }
  },
  "builds": [
    {
      "src": "src/http/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/http/server.ts"
    }
  ]
}