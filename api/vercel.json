{
  "version": 2,
  "builds": [
    {
      "src": "src/http/server.ts",
      "use": "@vercel/node",
      "config": {
        "generateBuildId": true
      }
    }
  ],
  "routes": [
    {
      "src": "/docs(.*)",
      "dest": "src/http/server.ts"
    },
    {
      "src": "/auth(.*)",
      "dest": "src/http/server.ts"
    },
    {
      "src": "/api(.*)",
      "dest": "src/http/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/http/server.ts"
    }
  ]
}