{
  "version": 2,
  "builds": [
    {
      "src": "build/http/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs(.*)",
      "dest": "build/http/server.js"
    },
    {
      "src": "/auth(.*)",
      "dest": "build/http/server.js"
    },
    {
      "src": "/api(.*)",
      "dest": "build/http/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "build/http/server.js"
    }
  ]
}